{
  "name": "legiscan-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for LegiScan API - Access legislative data from all 50 US states",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run tests/e2e.test.ts",
    "inspect": "npx @modelcontextprotocol/inspector node dist/index.js"
  },
  "keywords": [
    "mcp",
    "legiscan",
    "legislation",
    "bills",
    "congress",
    "state-legislature"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "typescript": "^5.3.2",
    "vitest": "^4.0.16"
  }
}
